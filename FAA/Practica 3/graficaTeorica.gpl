set title "Medio Burbuja"
set key top left vertical inside
set grid
set xlabel "Talla (n)"
set ylabel "Tiempo (OEs)"
set xrange [0:100]

Lineal(x) = x
Binaria(x) = 5+3*(log(x)/log(2))
Ternaria(x) = 10+2*(log(x)/log(3))
plot Lineal(x), Binaria(x), Ternaria(x)


replot
#pause -1 "Pulsa Enter para continuar..."
unset output